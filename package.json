{
  "name": "ngx-webpack-grande-starter",
  "version": "2.0.0",
  "description": "A grande-cup webpack starter for Angular 2+",
  "scripts": {
    "serve": "npm run serve:c",
    "serve:c": "cross-env NODE_ENV=development webpack-dev-server --config webpack/w.c.client.spa --hot --inline --progress",
    "build": "npm run build:c",
    "build:c": "cross-env NODE_ENV=production webpack --config webpack/w.c.client.spa --progress --profile --bail"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@ngtools/webpack": "^6.0.0",
    "@types/node": "^10.0.3",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.4",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "typescript": "~2.7.0",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.0",
    "webpack-dev-server": "^3.1.0",
    "webpack-merge": "^4.1.0"
  }
}
